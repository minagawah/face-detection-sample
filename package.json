{
  "name": "face-detection-sample",
  "version": "0.4.0",
  "description": "Playing with Face Detection API + React Hooks",
  "private": true,
  "scripts": {
    "watch:css": "postcss src/index.css -o build/index.css -w",
    "build:css": "postcss src/index.css -o build/index.css",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start": "BROWSER=none npm-run-all -p watch:css start:js",
    "build": "npm-run-all build:css build:js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": "https://github.com/minagawah/face-detection-sample.git",
  "homepage": "/minagawah/face-detection-sample",
  "keywords": [
    "Face Detection API",
    "React",
    "Hooks"
  ],
  "author": "minagawah <minagawa@tokyo800.jp>",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/minagawah/face-detection-sample/issues"
  },
  "dependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "react-use": "^12.2.0",
    "typescript": "^3.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.7.0",
    "tslint": "^5.19.0",
    "tslint-react": "^4.0.0"
  }
}
